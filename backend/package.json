{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "ENV=development nodemon app.js",
    "up": "npx knex migrate:latest --knexfile src/utils/database/knexfile.js",
    "down": "npx knex migrate:down --knexfile src/utils/database/knexfile.js",
    "rollback": "npx knex migrate:rollback --knexfile src/utils/database/knexfile.js",
    "seed": "npx knex seed:run --knexfile src/utils/database/knexfile.js ",
    "fill_with_tests": "ENV=test PORT=3000 npx knex seed:run --knexfile src/utils/database/knexfile.js",
    "test": "PORT=5000 npm run fill_with_tests && PORT=3000 mocha \"src/**/*.spec.js\" --exit"
  },
  "author": "Wojciech Gos",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "2.1227.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "express-async-errors": "3.1.1",
    "http-status-codes": "2.2.0",
    "jsonwebtoken": "9.0.0",
    "knex": "2.4.2",
    "nodemon": "2.0.19",
    "objection": "3.0.1",
    "passport": "0.6.0",
    "passport-google-oauth20": "2.0.0",
    "pg": "8.8.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "mocha": "10.1.0"
  }
}
