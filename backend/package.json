{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "ENV=development nodemon app.js",
    "make":"knex migrate:make schema2 --knexfile src/utils/database/knexfile.js",
    "up": "npx knex migrate:up 20230811113649_schema.js --knexfile src/utils/database/knexfile.js",
    "down": "npx knex migrate:down 20230811113649_schema.js --knexfile src/utils/database/knexfile.js ",
    "rollback": "npx knex migrate:rollback --all --knexfile src/utils/database/knexfile.js",
    "seed": "npx knex seed:run --knexfile src/utils/database/knexfile.js",
    "prepare_db": "npm run down && npm run up && npm run seed",
    "list": "npx knex migrate:list --knexfile src/utils/database/knexfile.js",
    "status": "npx knex migrate:status --knexfile src/utils/database/knexfile.js",
    "test": "PORT=5000 npm run prepare_db && PORT=3030 mocha \"src/**/*.spec.js\" --exit"
  },
  "author": "Wojciech Gos",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "2.1227.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "express-async-errors": "3.1.1",
    "http-status-codes": "2.2.0",
    "jsonwebtoken": "9.0.0",
    "knex": "2.5.1",
    "nodemon": "2.0.19",
    "objection": "3.0.1",
    "passport": "0.6.0",
    "passport-google-oauth20": "2.0.0",
    "pg": "8.8.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "mocha": "10.1.0"
  }
}
